{
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": ["error", 2],
        "no-param-reassign": ["error", { "props": false }],
        "quotes": ["error", "single"]
    },
    "overrides": [
        {
            "files": [
                "*.test.js"
            ],
            "env": {
                "jest": true
            }
        }
    ]
}
